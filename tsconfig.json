{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "es6"
    ],
    "useDefineForClassFields": true,
    "module": "es6",
    "rootDir": ".",
    "moduleResolution": "node",
    "resolveJsonModule": false,
    "allowJs": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "newLine": "lf",
    "stripInternal": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "files": [
    "memoml.ts"
  ]
}
